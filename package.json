{
  "name": "bugsplat-node",
  "version": "2.1.0",
  "description": "BugSplat integration for node.js 12+",
  "main": "./dist/index.js",
  "scripts": {
    "test": "ts-node node_modules/jasmine/bin/jasmine --config=spec/support/jasmine.spec.json",
    "test:teamcity": "ts-node node_modules/jasmine/bin/jasmine --config=spec/support/jasmine.teamcity.spec.json",
    "e2e": "ts-node node_modules/jasmine/bin/jasmine --config=spec/support/jasmine.e2e.json",
    "e2e:teamcity": "ts-node node_modules/jasmine/bin/jasmine --config=spec/support/jasmine.teamcity.e2e.json",
    "build": "tsc",
    "release": "npm run build && npm publish --access public"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "bugsplat",
    "crash",
    "reporting",
    "analytics",
    "stack",
    "trace",
    "error",
    "javascript",
    "node"
  ],
  "homepage": "https://www.bugsplat.com",
  "bugs": "https://github.com/BugSplat-Git/bugsplat-node/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/BugSplat-Git/bugsplat-node"
  },
  "author": "@bobbyg603",
  "license": "MIT",
  "dependencies": {
    "bugsplat": "^7.1.3"
  },
  "devDependencies": {
    "@bugsplat/js-api-client": "^2.0.0",
    "@types/jasmine": "^4.0.3",
    "@types/node": "^16.11.47",
    "jasmine": "^4.3.0",
    "jasmine-reporters": "^2.5.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  }
}
