{
  "name": "bugsplat-node",
  "version": "1.1.1",
  "description": "BugSplat integration for node.js 12+",
  "main": "bugsplat-node.js",
  "scripts": {
    "test": "jasmine --config=spec/support/jasmine.json",
    "test:teamcity": "jasmine --config=spec/support/jasmine.teamcity.json",
    "e2e": "jasmine --config=spec/support/jasmine.e2e.json",
    "e2e:teamcity": "jasmine --config=spec/support/jasmine.teamcity.e2e.json",
    "dist": "mkdir  -p ./dist && cp ./{bugsplat-node.js,bugsplat-node.d.ts,package.json,README.md} ./dist"
  },
  "keywords": [
    "bugsplat",
    "crash",
    "reporting",
    "analytics",
    "stack",
    "trace",
    "error",
    "javascript",
    "node"
  ],
  "homepage": "https://www.bugsplat.com",
  "bugs": "https://github.com/BugSplat-Git/bugsplat-node/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/BugSplat-Git/bugsplat-node"
  },
  "author": "@bobbyg603",
  "license": "MIT",
  "dependencies": {
    "@types/node": "^14.6.0",
    "bugsplat": "^7.0.2"
  },
  "devDependencies": {
    "@bugsplat/js-api-client": "^0.0.6",
    "jasmine": "^3.6.1",
    "jasmine-reporters": "^2.4.0"
  }
}
